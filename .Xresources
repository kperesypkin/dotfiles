!Urxvt settings
URxvt.title:             Terminal
URxvt.iconFile:          /usr/share/icons/Yaru/48x48/apps/terminal-app.png
URxvt.depth:		     24
URxvt.termName:          xterm-256color
Urxvt.buffered: 		 True
URxvt.font:     		 xft:Fira Code:pixelsize=15:antialias=true:hinting=true
URxvt.boldFont: 		 xft:Fira Code:bold:pixelsize=15:antialias=true:hinting=true
URxvt.italicdFont: 		 xft:Fira Code:italic:pixelsize=15:antialias=true:hinting=true
URxvt.boldItalicFont: 	 xft:Fira Code:bold italic:pixelsize=15:antialias=true:hinting=true
URxvt.scrollBar:		 false
URxvt.saveLines:		 65535
URxvt.letterSpace:		 -1
URxvt.secondaryScreen:	 1
URxvt.secondaryScroll:	 0
URxvt.scrollTtyOutput:	 false
URxvt.scrollWithBuffer:	 true
URxvt.scrollTtyKeypress: true
URxvt.urgentOnBell:      false
URxvt.visualBell:        false
URxvt.cursorBlink:       true
URxvt.cursorUnderline:   true
URxvt.cursorColor:       #F92660
URxvt.fading:            20
URxvt.colorMode:         on
URxvt.boldColors:        on
URxvt.colorUL:           #E6DB74 
URxvt.underlineColor:    #E6DB74
URxvt.url-launcher:		 /usr/bin/firefox
URxvt.underlineURLs:	 true
Xft*dpi:       96
Xft*antialias: True
Xft*hinting:   True
Xft*hintstyle: hintfull
Xft*lcdfilter: lcddefault
